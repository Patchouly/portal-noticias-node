<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html>
<html lang="pt-br">
	<head>
		<meta charset="utf-8"/>
		<title>Portal de Notícias</title>
	</head>
	<body>

		<h1>Notícias</h1>

		<table>
			<tr>
				<td>ID</td>
				<td>Titulo</td>
				
			</tr>
			<% for(var i=0; i < noticias.length; i++) { %>
			<tr>
				<td><%= noticias[i].id %></td>
				<td><%= noticias[i].titulo %></td>
			</tr>
			<% } %>
		</table>
		
	</body>
</html>